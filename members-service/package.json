{
  "name": "sumofus-member-service",
  "version": "0.1.0",
  "description": "Member service. Mainly interfaces with ActionKit.",
  "scripts": {
    "nyc": "nyc",
    "test": "jest"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "jest": "^21.2.1",
    "nyc": "^11.3.0",
    "serverless-webpack": "^3.1.1",
    "webpack": "^3.3.0"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT",
  "dependencies": {
    "axios-es6": "^0.11.1",
    "lodash": "^4.17.4",
    "rest": "^2.0.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false,
          "targets": {
            "node": "6.10"
          }
        }
      ]
    ],
    "plugins": [
      "transform-flow-strip-types",
      "transform-object-rest-spread"
    ],
    "env": {
      "test": {
        "presets": [
          [
            "env",
            {
              "modules": false,
              "targets": {
                "node": "6.10"
              }
            }
          ]
        ],
        "plugins": [
          "transform-flow-strip-types",
          "transform-object-rest-spread",
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
