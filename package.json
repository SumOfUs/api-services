{
  "name": "api-services",
  "version": "0.1.0",
  "description": "SumOfUs serverless services",
  "main": "index.js",
  "scripts": {
    "ci:test": "jest --coverage",
    "ci:coveralls": "cat coverage/lcov.info | coveralls",
    "precommit": "lint-staged",
    "flow": "flow",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SumOfUs/api-services.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SumOfUs/api-services/issues"
  },
  "homepage": "https://github.com/SumOfUs/api-services#readme",
  "devDependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "dotenv": "^4.0.0",
    "flow-aws-lambda": "^1.0.0",
    "flow-bin": "^0.59.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^4.3.0",
    "prettier": "^1.8.2",
    "serverless-webpack": "^3.1.1",
    "webpack": "^3.3.0"
  },
  "dependencies": {
    "braintree": "^2.4.0",
    "guid": "0.0.12",
    "lodash": "^4.17.4",
    "replayer": "^2.1.3",
    "source-map-support": "^0.5.0"
  },
  "prettier": {
    "parser": "babylon",
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false,
          "targets": {
            "node": "6.10"
          }
        }
      ]
    ],
    "plugins": [
      "lodash",
      "transform-flow-strip-types",
      "transform-object-rest-spread"
    ],
    "env": {
      "test": {
        "presets": [
          [
            "env",
            {
              "modules": false,
              "targets": {
                "node": "6.10"
              }
            }
          ]
        ],
        "plugins": [
          "transform-flow-strip-types",
          "transform-object-rest-spread",
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/node_modules/dotenv/config",
      "<rootDir>/jest.setup.js"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
